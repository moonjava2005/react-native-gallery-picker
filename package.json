{
  "name": "react-native-gallery-picker",
  "version": "1.0.25",
  "description": "Support select image, video from gallery with iCloud",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "/ios",
    "/android",
    "/RNGalleryPicker.podspec"
  ],
  "scripts": {
    "dist-compile": "BABEL_ENV=production babel . --ignore node_modules,dist --out-dir dist",
    "test": "yarn lint && jest",
    "lint": "eslint --ext '.js,.ts,.tsx' .",
    "typescript": "tsc",
    "fix": "eslint --fix --ext '.js,.ts,.tsx' .",
    "build": "yarn typescript",
    "contrib:add": "all-contributors add",
    "contrib:generate": "all-contributors generate",
    "contrib:check": "all-contributors check",
    "clear_jest": "jest --clearCache",
    "clean": "rm -rf lib",
    "prepublish": "rm -rf android/build"
  },
  "author": {
    "name": "MoonJava",
    "email": "moonjava@gmail.com"
  },
  "keywords": [
    "react-native",
    "react-component",
    "ios",
    "android",
    "picker",
    "cloud",
    "crop",
    "gallery",
    "cameraroll"
  ],
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "prettier": "^2.0.5",
    "typescript": "4.0.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "./"
    ]
  },
  "homepage": "https://github.com/moonjava2005/react-native-gallery-picker",
  "readme": "https://github.com/moonjava2005/react-native-gallery-picker#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/moonjava2005/react-native-gallery-picker.git"
  }
}
